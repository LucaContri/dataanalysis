<web-app version="3.0"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <servlet-name>ResourcesServlet</servlet-name>
    <servlet-class>com.saiglobal.reporting.servlets.ResourcesServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AuditDaysSumamryServlet</servlet-name>
    <servlet-class>com.saiglobal.reporting.servlets.AuditDaysSumamryServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DailyStatsServlet</servlet-name>
    <servlet-class>com.saiglobal.reporting.servlets.DailyStatsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DailyStatsParametersServlet</servlet-name>
    <servlet-class>com.saiglobal.reporting.servlets.DailyStatsParametersServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>KPIServlet</servlet-name>
    <servlet-class>com.saiglobal.reporting.servlets.KPIServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ProcessDetailsARGServlet</servlet-name>
    <servlet-class>com.saiglobal.reporting.servlets.ProcessDetailsARGServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ProcessDetailsARGServletV2</servlet-name>
    <servlet-class>com.saiglobal.reporting.servlets.ProcessDetailsARGServletV2</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ProcessParametersServlet</servlet-name>
    <servlet-class>com.saiglobal.reporting.servlets.ProcessParametersServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CompassRolloutProgressServlet</servlet-name>
    <servlet-class>com.saiglobal.reporting.servlets.CompassRolloutProgressServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>KPI2Servlet</servlet-name>
    <servlet-class>com.saiglobal.reporting.servlets.KPI2Servlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ExecutiveDashboardServlet</servlet-name>
    <servlet-class>com.saiglobal.reporting.servlets.ExecutiveDashboardServlet</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>ReportServlet</servlet-name>
    <servlet-class>com.saiglobal.reporting.servlets.ReportServlet</servlet-class>
  </servlet>
     <servlet>
    <servlet-name>AllocatorOutputServlet</servlet-name>
    <servlet-class>com.saiglobal.reporting.servlets.AllocatorOutputServlet</servlet-class>
  </servlet>
     <servlet>
    <servlet-name>AllocatorOutputTableServlet</servlet-name>
    <servlet-class>com.saiglobal.reporting.servlets.AllocatorOutputTableServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AllocatorOutputTableServlet</servlet-name>
    <url-pattern>/allocatorOutputTableServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AllocatorOutputServlet</servlet-name>
    <url-pattern>/allocatorOutputServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ReportServlet</servlet-name>
    <url-pattern>/reportServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ExecutiveDashboardServlet</servlet-name>
    <url-pattern>/executiveDashboard</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>KPI2Servlet</servlet-name>
    <url-pattern>/kpiv2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CompassRolloutProgressServlet</servlet-name>
    <url-pattern>/compassRolloutProgress</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProcessParametersServlet</servlet-name>
    <url-pattern>/processParameters</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProcessDetailsARGServlet</servlet-name>
    <url-pattern>/argprocessdetails</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProcessDetailsARGServletV2</servlet-name>
    <url-pattern>/argprocessdetailsv2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>KPIServlet</servlet-name>
    <url-pattern>/kpi</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DailyStatsParametersServlet</servlet-name>
    <url-pattern>/dailyStatsParameters</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DailyStatsServlet</servlet-name>
    <url-pattern>/dailyStats</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AuditDaysSumamryServlet</servlet-name>
    <url-pattern>/auditDaysSummary</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ResourcesServlet</servlet-name>
    <url-pattern>/resources</url-pattern>
  </servlet-mapping>
  
  <resource-ref>
	<description>MySql - Compass data copy</description>
	<res-ref-name>jdbc/compass</res-ref-name>
	<res-type>javax.sql.DataSource</res-type>
	<res-auth>Container</res-auth>
  </resource-ref>

</web-app>